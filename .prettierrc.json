{
	"printWidth": 180,
	"tabWidth": 4,
	"useTabs": true,
	"semi": false,
	"vueIndentScriptAndStyle": false,
	"singleQuote": true,
	"bracketSpacing": true,
	"trailingComma": "none",
	"jsxBreaketSameLine": true,
	"jsxSingleQuote": false,
	"arrowParens": "avoid",
	"insertPragma": false,
	"requirePragma": false,
	"proseWrap": "never",
	"htmlWhitespaceSensitivity": "ignore",
	"endOfLine": "auto",
	"rangeStart": 0
}
